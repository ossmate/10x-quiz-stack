# Supabase Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_KEY=your_supabase_anon_key

# Service Role Key - WARNING: Use only for internal jobs/scripts, NOT for HTTP routes
# All HTTP routes enforce Row-Level Security (RLS) policies
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# OpenRouter AI Configuration
OPENROUTER_API_KEY=your_openrouter_api_key

# AI Configuration (Optional - defaults provided)
AI_MODEL=gpt-4
AI_TEMPERATURE=0.7
AI_MAX_TOKENS=2000
AI_ENABLE_USAGE_LOGGING=true

# Vercel Configuration (automatically set by Vercel)
VERCEL_URL=
VERCEL_ENV=

# E2E Testing Configuration (for Playwright tests)
# Create a test user in your Supabase project and add credentials here
TEST_USER_EMAIL=test@example.com
TEST_USER_PASSWORD=test_password_123